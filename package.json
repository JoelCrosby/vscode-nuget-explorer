{
  "name": "nuget-explorer",
  "displayName": "NuGet Explorer",
  "description": "",
  "version": "0.0.1",
  "publisher": "joel-crosby",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csharp",
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "nuget-explorer",
          "title": "Nuget Explorer",
          "icon": "media/nuget.svg"
        }
      ]
    },
    "views": {
      "nuget-explorer": [
        {
          "id": "nuget-installed",
          "name": "Installed Packages"
        }
      ]
    },
    "commands": [
      {
        "command": "nuget-explorer.refresh",
        "title": "NuGet: Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/xmldom": "^0.1.29",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "xmldom": "~0.1.27"
  }
}
